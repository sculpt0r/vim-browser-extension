{
	"name": "VIM typer",
	"version": "1.0.3",
	"description": "Use VIM keys in browser",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": [
				"https://*/*",
				"http://*/*",
				"file://*/*"
			],
			"js": [
				"./plugin.js"
			],
			"run_at": "document_start",
			"all_frames": true
		}
	]
}
