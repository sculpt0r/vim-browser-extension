{
    "name": "VIM typer",
    "version": "1.0",
    "description": "Use VIM keys in browser",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": [ "<all_urls>" ],
        "js": [ "./carret.js", "./myforeground.js" ],
        "run_at": "document_start",
        "all_frames": true
      }
    ]
   
}